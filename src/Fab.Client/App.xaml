<ApplicationModel:CaliburnApplication x:Class="Fab.Client.App"
									  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
									  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
									  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
									  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
									  xmlns:ApplicationModel="clr-namespace:Caliburn.PresentationFramework.ApplicationModel;assembly=Caliburn.PresentationFramework"
									  xmlns:Windows="clr-namespace:Fab.Client.Common"
									  mc:Ignorable="d">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Resources/Primitives.xaml" />
				<ResourceDictionary Source="Resources/AutoCompleteComboBox.xaml" />
				<ResourceDictionary Source="Themes/generic.xaml" />
				<ResourceDictionary Source="Resources/BS_ResourceDictionary_Basic.xaml" />
				<ResourceDictionary Source="Resources/BS_ResourceDictionary_Advanced.xaml" />
				<ResourceDictionary Source="Resources/BS_ResourceDictionary_Brushes.xaml" />
			</ResourceDictionary.MergedDictionaries>
			<Windows:CultureSettings x:Key="CultureSettings" />
		</ResourceDictionary>
	</Application.Resources>
</ApplicationModel:CaliburnApplication>